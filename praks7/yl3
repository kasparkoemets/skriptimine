#!/bin/bash
#
#aja kohaselt tervitamine
#
echo "Sisesta kellaaeg:"
read aeg
if [ $aeg -ge 6 -a $aeg -lt 12 ]; then 
echo "Tere Hommikust!"
elif [ $aeg -ge 12 -a $aeg -lt 18 ]; then
echo "Tere PÃ¤evast!"
elif [ $aeg -ge 18 -a $aeg -lt 22 ]; then
echo "Tere Ohtust!"
elif [ $aeg -ge 22 -a $aeg -lt 24 ] || [ $aeg -ge 0 -a $aeg -lt 6]; then
echo "Head Ood!"
else
echo "Vale sisend"
fi
